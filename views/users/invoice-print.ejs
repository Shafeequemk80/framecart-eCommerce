  <body class="bg-white" onload="printPromot()">
    <div class="nk-block">
      <div class="invoice invoice-print">
        <div class="invoice-wrap">
          <div class="invoice-brand text-center">
            <img
              src="images/logo-dark.png"
              srcset="/demo2/images/logo-dark2x.png 2x"
              alt=""
            />
          </div>
          <div class="invoice-head">
            <div class="invoice-contact">
              <span class="overline-title">Invoice To</span>
              <div class="invoice-contact-info">
                <h4 class="title"><%=orderData.address.fullname%></h4>
                <ul class="list-plain">
                  <li>
                    <em class="icon ni ni-map-pin-fill fs-18px"></em
                    ><span
                      ><%=orderData.address.address%>,<%=orderData.address.locality%> <br> <%=orderData.address.city%>, <%=orderData.address.state%>,<%=orderData.address.zipcode%>
                      </span
                    >
                  </li>
                  <li>
                    <em class="icon ni ni-call-fill fs-14px"></em
                    ><span><%=orderData.address.mobile%></span>
                  </li>
                </ul>
              </div>
            </div>
            <div class="invoice-desc">
              <h3 class="title">Invoice</h3>
              <ul class="list-plain">
                <li class="invoice-id">
                  <span>Invoice ID</span>:<span><%=orderData.orderId%></span>
                </li>
                <li class="invoice-date">
                  <span>Date</span>:<span><%=moment(orderData.orderDate).format('DD MMM YYYY');%></span>
                </li>
              </ul>
            </div>
          </div>
          <div class="invoice-bills">
            <div class="table-responsive">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th class="w-150px">Item ID</th>
                    <th class="w-40">Product Name</th>
                    <th class="w-20">Frame shape</th>
                    <th>Price</th>
                    <th>Qty</th>
                    <th>Amount</th>
                  </tr>
                </thead>
                <tbody>
                 <%orderData.products.forEach((value,i)=>{%>
                  <tr>
                    <td><%= String(value.product._id).substring(0, 6).toUpperCase() %> </td>
                    <td><%=value.product.productname%></td>
                    <td><%=value.product.frameshape%></td>
                    <td>  <%=value.totalprice%></td>
                    <td><%=value.count%></td>
                    <td><%=value.totalprice*value.count %></td>
                  </tr>
                  <%})%> 
                </tbody>
                <tfoot>
                  <tr>
                    <td colspan="3"></td>
                    <td colspan="2">Subtotal</td>
                    <td><%=orderData.amount%></td>
                  </tr>
                  <tr>
                    <td colspan="3"></td>
                    <td colspan="2">Processing fee</td>
                    <td>free</td>
                  </tr>
                  
                  <tr>
                    <td colspan="3"></td>
                    <td colspan="2">Grand Total</td>
                    <td><%=orderData.amount%></td>
                  </tr>
                </tfoot>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script>
      function printPromot() {
        window.print();
      }
    </script>
  </body>
